pipeline {
    agent any

    stages {
        stage('Clone Code') {
            steps {
                echo "Code is cloned automatically from GitHub"
            }
        }

        stage('Build') {
            steps {
                echo "Building the project..."
            }
        }

        stage('Test') {
            steps {
                echo "Running tests..."
            }
        }

        stage('Deploy') {
            steps {
                echo "Deploying application..."
            }
        }

        stage('Security Check') {
            steps {
                echo "Performing security scan..."
            }
        }
    }
}



